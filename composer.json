{
    "name": "jeffersonceo/aplugin", 
    "type": "library",
    "license": "MIT",

    "require": {
        "illuminate/support": "^10.0",
        "symfony/process": "^6.0",
        "illuminate/console": "^10.0",
        "illuminate/filesystem": "^10.0",
        "illuminate/view": "^10.0",
        "illuminate/auth": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\Plugins\\": "src/"
        }
    },
    "autoload-dev": {
